<div class="container">
	<div class="row">
		<div class="col-lg-12">
			<div class="page-header">
				<h1>
				Edit Submission
				<%= link_to submission_path(@submission), method: :delete, class: "pull-right", data: {confirm: "Are you sure you want to delete this?"} do %>
				<i class="fa fa-trash-o" aria-hidden="true"></i>
				<%end%>
				</h1>
			</div>
		</div>
		<div class="col-lg-offset-3 col-lg-6 col-md-offset-3 col-md-6">
			<%= form_for @submission, method: :patch do |f| %>
			<div class="form-group">
				<%= f.label :name, class: "control-label" %>:
				<%= f.text_field :name, class: "form-control", disabled: "" %>
			</div>
			<div class="form-group">
				<%= f.label :contribution %>:
				<%= f.text_area :contribution, rows:5, class:"form-control" %>
			</div>
			<%= f.submit 'Submit', class:"btn btn-primary" %>
			<%end%>
		</div>
	</div>
</div>